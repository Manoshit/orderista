<% include partials/header %>
	<div style="background-color: #f2f3f7;">
		<div class="container" >
			<div class="row">
				<div class="col-md-12" style="padding-top: 20px;">
					<div id="contact-cover">
						<p id="contact-text">WE WOULD LOVE TO HEAR FROM YOU</p>
					</div>

				</div>
			</div>
			<div class="row">
				<div class="col-md-8">
					<div class="card" id="card">
						<form id="form" action='/contact' method='post'>
							<div class="form-group">
								<input type="text" name="name" class="form-control" placeholder="NAME">
							</div>
							<div class="form-group">
								<input type="email" name="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="EMAIL">
							</div>

							<div class="form-group">
								<textarea  placeholder="WRITE A MESSAGE" name="subject" class="form-control"  rows="10">
								</textarea>


							</div>

							<button type="submit" class="btn btn-dark">Send Message</button>
						</form>
					</div>
				</div>
				<div class="col-md-4">
					<div class="card">
						<div class="card-body">
							<h5 class="text1">PARTNER WITH US</h5>



							<p class="text1">OWN A RESTAURANT? SEE HOW YOU CAN GET MOST OF IT</p>


							<a href="/register" class="btn btn-dark" class="text1">REGISTER YOUR RESTAURANT</a>
						</div>
					</div>

				</div>

			</div>
		</div>
	</div>
<% include partials/footer %>